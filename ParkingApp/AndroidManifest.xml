<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ywx.parkingapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    
    <uses-permission android:name="android.permission.INTERNET" />
   <!--  <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" /> -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        
        <activity
            android:name="com.ywx.parkingapp.SplashActivity"
            android:label="@string/title_activity_parking_map" >
            <intent-filter> 
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>     
        </activity>   
        
        <activity
            android:name="com.ywx.parkingapp.LoginActivity"
            android:label="@string/app_name" > 
        </activity>
        
        <activity
            android:name="com.ywx.parkingapp.UserSignupActivity"
            android:label="@string/title_activity_user_signup" >
        </activity>
        
        <activity
            android:name="com.ywx.parkingapp.ParkingMapActivity"
            android:label="@string/title_activity_parking_map" >
        </activity> 
        
